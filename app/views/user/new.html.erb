<%= form_for @user, url: {action: "create"} do |f| %>
  <%= f.label :username, "Username:" %>
  <%= f.text_field :username %><br/>
  <%= f.label :email, "Email:" %>
  <%= f.email_field :email %><br/>
  <%= f.label :password, "Password:" %>
  <%= f.password_field :password%><br/>
  <%= f.submit "Submit" %>
<% end %>

